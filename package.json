{
  "name": "typescript-slides",
  "private": true,
  "scripts": {
    "build": "slidev build --base=/typescript/",
    "build:debrief": "slidev build --base=/ --out=dist-debrief",
    "dev": "slidev --open",
    "export": "slidev export",
    "deploy": "aws s3 rm --recursive s3://drive.delicious-insights.com/typescript && aws s3 cp --recursive --acl=public-read dist s3://drive.delicious-insights.com/typescript/"
  },
  "dependencies": {
    "@slidev/cli": "^0.41.1",
    "@slidev/theme-default": "*",
    "@slidev/theme-seriph": "*"
  },
  "devDependencies": {
    "@iconify-json/logos": "^1.1.31",
    "@iconify-json/mdi": "^1.1.52",
    "@types/react": "^18.2.6",
    "playwright-chromium": "^1.34.0",
    "react": "^18.2.0",
    "ts-expect": "^1.3.0"
  }
}
